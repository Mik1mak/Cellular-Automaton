# Ilości aktywnych sąsiednich pól, przy których komórka nieaktywna
#  zmienia stan na aktywną (odseparowane przecinkami)
bornConditions=3
#
# Ilości aktywnych sąsiednich pól, przy których komórka aktywna
#  nie zmienia stanu (odseparowane przecinkami)
surviveConditions=2,3
#
# Ilość iteracji do przeprowadzenia przez program
iter=512
#
# Rozmiar pola (podawany jako długość boku kwadratu w pikselach)
fieldSize=1
#
# Rozmiar układu (podawany jako długość boku układu w polach)
boardSize=64
#
# tryb danych wyjścowych zapisany jako suma wybranych opcji
# 1. rysowanie stanów układu we wszystkich iteracjach jako grafiki
#   (frame*.bmp)
# 2. rysowanie finalnego stanu układu jako grafiki (output.bmp)
# 4. wyświetlanie finalnego stanu w konsoli (gdzie '_' reprezentuje
#  komórkę nieaktywną a '@' aktywną)
# 8.rysowanie finalnego stanu układu do pliku tekstowego (output.txt)
outputMode=10
#
# Stan początkowy układu gdzie każda atywna komórka zapisana jest jako
# "kolumna-wiersz," gdzie indeksy zawierają się w <0, size)
init=25-1,25-2,23-2,13-3,14-3,21-3,22-3,35-3,36-3,12-4,16-4,21-4,22-4,35-4,36-4,1-5,2-5,11-5,17-5,21-5,22-5,1-6,2-6,11-6,15-6,17-6,18-6,23-6,25-6,11-7,17-7,25-7,12-8,16-8,13-9,14-9,